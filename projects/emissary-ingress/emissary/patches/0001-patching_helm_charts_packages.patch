+        - name: emissary-apiext
+          image: {{ .Values.sourceRegistry }}/{{ include "image.repository.path" .Values.image }}
+          imagePullPolicy: Always
+          command: [ "apiext", "emissary-apiext" ]
+          ports:
+            - name: http
+              containerPort: 8080
+            - name: https
+              containerPort: 8443
