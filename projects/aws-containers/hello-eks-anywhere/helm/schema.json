{
  "$id": "https://hello-eks-anywhere.packages.eks.amazonaws.com/schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "hello-eks-anywhere",
  "type": "object",
  "properties": {
    "namespace": {
      "type": "string",
      "default": "default",
      "description": "Namespace for package and resources."
    },
    "sourceRegistry": {
      "type": "string",
      "default": "public.ecr.aws/eks-anywhere",
      "description": "Source registry for package."
    },
    "repository": {
      "type": "string",
      "default": "hello-eks-anywhere",
      "description": "Repository for package."
    },
    "imagePullSecrets": {
      "type": "array",
      "default": [],
      "description": "Container imagePullSecrets.",
      "items": {
        "type": "object"
      }
    },
    "tag": {
      "type": "string",
      "description": "Container image tag."
    },
    "title": {
      "type": "string",
      "default": "Amazon EKS Anywhere",
      "description": "Container title."
    },
    "subtitle": {
      "type": "string",
      "default": "Run EKS in your datacenter",
      "description": "Container subtitle."
    }
  },
  "additionalProperties": false
}
