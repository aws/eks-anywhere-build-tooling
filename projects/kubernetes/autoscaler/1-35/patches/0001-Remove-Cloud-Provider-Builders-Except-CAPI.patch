 	case cloudprovider.ClusterAPIProviderName:
 		return clusterapi.BuildClusterAPI(opts, do, rl)
-	case cloudprovider.IonoscloudProviderName:
-		return ionoscloud.BuildIonosCloud(opts, do, rl)
